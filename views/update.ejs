<!DOCTYPE html>
<html>
  <head>

  </head>
  <body>
<table>
  <tr>
    
      <th>Title</th>
      <th>Author</th>
      <th>Genre</th>
    
  </tr>
      <% for(var i=0;i< arrVal.length ; i++) { %>
          <form action="/updated" method="POST">
            <tr>
            <td><input hidden type="text" name="preTitle" value="<%=arrVal[i].title%>"></td>
            <td><input type="text" name="title" value="<%=arrVal[i].title%>"></td>
            <td><input type="text" name="author"  value="<%=arrVal[i].author%>"></td>
            <td><input type="text" name="genre"  value="<%=arrVal[i].genre%>"></td> 
            <td><input type="submit" value="update"></td>
          </tr>
        </form>
          
        
      <% } %>
      
    </table>


</html>
